%{
#include <stdio.h>
#include <stdlib.h>
#include "y.tab.h"  //generated by yacc -d

%}

%%

int|double|string|float|char {return datatype;}
[0-9] {return num;}
[_a-zA-Z][_a-zA-Z0-9]* {return varname;}
"+"|"-"|"*"|"/" {return operator;}
"=" {return eq;}
";" {return sc;}

%%

int yywrap()      {return -1;}

